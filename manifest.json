{
   "name": "Facebook birthdays sync",
   "description" : "Sync facebook friends' birthdays to google calendar",
   "version": "1.0",
   "manifest_version": 2,
   "browser_action": {
     "default_icon": "logo.jpg"
   },
   "permissions": [
     "activeTab",
     "tabs",
     "https://www.facebook.com/"
   ],

   "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
 }
